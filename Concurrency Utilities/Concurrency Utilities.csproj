<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<PackageId>ConcurrencyUtilities</PackageId>
		<Version>1.0.2</Version>
		<Authors>Momen Shaker</Authors>
		<Description>A library for simplifying concurrent and parallel processing in .NET.</Description>
		<PackageTags>concurrency, parallel, threading, caching</PackageTags>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RepositoryUrl>https://github.com/momenshaker/Concurrency-Utilities</RepositoryUrl>

	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.0" />
	</ItemGroup>

</Project>
